
<section data-ng-init="getAllRelationship()">
    <div class="btn-toolbar">
        <div class="btn-group">
            <button data-ng-disabled="isPublished=='False'" title='@T("Works when the entity has been published.")'
                class="btn btn-small btn-success dropdown-toggle" data-toggle="dropdown">
                <i class="icon-plus"></i>&nbsp;@T("Add") <span class="caret"></span>
            </button>
            <ul class="dropdown-menu">
                <li><label data-ng-click="createOneToMany()"><i class="icon-plus"></i>&nbsp;@T("One-to-Many Relationship")</label></li>
                <li><label data-ng-click="createManyToMany()"><i class="icon-plus"></i>&nbsp;@T("Many-to-Many Relationship")</label></li>
            </ul>
        </div>
    </div>
    <div class="row-fluid" data-ag-grid="gridOptions" data-ag-grid-name="relationList"> </div>
</section>
