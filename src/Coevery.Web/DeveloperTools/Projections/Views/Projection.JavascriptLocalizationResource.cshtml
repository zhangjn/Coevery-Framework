
@using (Script.Foot()) {
    <script>
      
        angular.module('jm.i18next').config(['$i18nextProvider', function ($i18nextProvider) {
            var translations = {
                'Prev': '@T("Prev")',
                'Next': '@T("Next")'
            };
            var resources = {
                dev: { translation: translations }
            };
            
            $.extend(true, $i18nextProvider.options, { resStore: resources });
        }]);

    </script>
}

