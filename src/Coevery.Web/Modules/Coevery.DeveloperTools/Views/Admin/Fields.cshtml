<div class="btn-toolbar">
    <button class="btn btn-small btn-success" ng-click="add()">
        <i class="icon-plus"></i>&nbsp;New Field
    </button>
    @*<button class="btn btn-small" ng-click="gotoDependency()">Field Dependency</button>*@
    <div class="btn-group" ng-show="selectedItems.length > 0">
        <button class="btn btn-small" data-ng-click="edit(selectedItems[0])" ng-disabled="selectedItems.length > 1">
            <i class="icon-edit"></i>&nbsp;Edit
        </button>
        <button class="btn btn-small" co-delete-button confirm-message="You really want to delete this field?" delete-action="delete()">
            <i class="icon-remove"></i>&nbsp;Delete
        </button>
    </div>
</div>
<div class="row-fluid">
     <div class="row-fluid" data-ag-grid="gridOptions" data-ag-grid-name="fieldList"> </div>
</div>

<div co-wizard wizard-options="wizardOptions"></div>
